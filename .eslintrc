{
  "extends": [ "airbnb/base" ],
  "env": { "node": true, "es6": true, "browser": true },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": false,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": false,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": false,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": false,
      "regexUFlag": false,
      "regexYFlag": false,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodePointEscapes": true,
      "globalReturn": false,
      "jsx": true,
      "experimentalDecorators": true
    }
  },
  "plugins": [ "babel", "import" ],
  "rules": {
    "array-bracket-spacing": [ 1, "always" ],
    "arrow-parens": [ 1, "as-needed" ],
    "brace-style": [ 1, "1tbs", { "allowSingleLine": true } ],
    "camelcase": 0,
    "class-methods-use-this": 0,
    "comma-dangle": [ 1, "never" ],
    "comma-spacing": [ 1, { "before": false, "after": true } ],
    "comma-style": [ 1, "last" ],
    "computed-property-spacing": 0,
    "consistent-return": 0,
    "consistent-this": 0,
    "constructor-super": 1,
    "curly": [ 1, "multi" ],
    "eol-last": 0,
    "func-names": 1,
    "func-style": 0,
    "generator-star-spacing": 0,
    "indent": [ 1, 2, { "MemberExpression": 1, "SwitchCase": 1 } ],
    "key-spacing": [ 1, { "beforeColon": false, "afterColon": true } ],
    "linebreak-style": 0,
    "max-nested-callbacks": [ 0, 3 ],
    "new-cap": 0,
    "new-parens": 1,
    "newline-after-var": 0,
    "no-array-constructor": 1,
    "no-cond-assign": [ 1, "except-parens" ],
    "no-console": 0,
    "no-constant-condition": 0,
    "no-continue": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 0,
    "no-empty": [ 1, { "allowEmptyCatch": true } ],
    "no-empty-character-class": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-func-assign": 1,
    "no-inline-comments": 0,
    "no-inner-declarations": [ 1, "functions" ],
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [ 1, { "max": 2, "maxEOF": 0, "maxBOF": 0 } ],
    "no-negated-in-lhs": 1,
    "no-nested-ternary": 0,
    "no-new-object": 1,
    "no-obj-calls": 1,
    "no-plusplus": 0,
    "no-regex-spaces": 1,
    "no-reserved-keys": 0,
    "no-restricted-globals": 0,
    "no-return-await": 0,
    "no-shadow": 0,
    "no-spaced-func": 1,
    "no-sparse-arrays": 1,
    "no-ternary": 0,
    "no-this-before-super": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 1,
    "no-unneeded-ternary": 1,
    "no-unreachable": 1,
    "no-var": 1,
    "object-curly-newline": [ 1, { "multiline": true } ],
    "object-curly-spacing": [ 1, "always" ],
    "object-property-newline": 1,
    "one-var": [ 1, "never" ],
    "operator-assignment": [ 1, "always" ],
    "padded-blocks": [ 0, "never" ],
    "prefer-const": [ 1, { "destructuring": "all" } ],
    "prefer-promise-reject-errors": 0,
    "quote-props": [ 1, "as-needed" ],
    "quotes": [ 1, "single" ],
    "semi": [ 1, "never" ],
    "semi-spacing": [ 1, { "before": false, "after": true } ],
    "sort-vars": 0,
    "space-before-function-paren": [
      1,
      { "anonymous": "never", "named": "never", "asyncArrow": "always" }
    ],
    "max-len": [
      1,
      {
        "code": 80,
        "tabWidth": 2,
        "ignoreTemplateLiterals": true,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ],
    "no-unused-expressions": 2,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": [ 2, { "devDependencies": true } ],
    "import/no-named-as-default": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "no-mixed-operators": 0,
    "function-paren-newline": [ 2, "consistent" ],
    "no-await-in-loop": 0,
    "no-restricted-syntax": 0
  }
}